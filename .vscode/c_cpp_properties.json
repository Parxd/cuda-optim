{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                // CUTLASS includes
                "${workspaceFolder}/cutlass/include",
                "${workspaceFolder}/cutlass/tools/util/include",
                "${workspaceFolder}/cutlass/examples/common"
            ],
            "defines": [
                "__CUDACC_VER_MAJOR__=12",
                "__CUDACC_VER_MINOR__=5"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}